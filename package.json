    "@types/redux-actions": "^2.6.1",
    "@types/redux-form": "^7.4.16",
    "@types/redux-mock-store": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "redux-mock-store": "^1.5.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack-cli": "^3.2.3"
  }
