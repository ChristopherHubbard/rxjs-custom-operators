    "@types/redux-form": "^7.4.16",
    "@types/redux-mock-store": "^1.0.0",
    "@types/sinon": "^7.0.6",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "redux-mock-store": "^1.5.3",
    "sinon": "^7.2.4",
    "typescript": "^3.3.1",
    "webpack-cli": "^3.2.3"
  }
