  "description": "RxJS custom operators to be imported as a package",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build-dev": "tsc -p ./ && NODE_ENV=LOCAL DEBUG_MODE=TRUE webpack --config ./distlib/webpack.config.js --mode development",
    "build-prod": "tsc -p ./ && NODE_ENV=PROD webpack --config ./distlib/webpack.config.js --mode production",
    "test": "mocha -r ts-node/register ./src/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rxjs-custom-operators.git"
  },
