    "url": "https://github.com/ChristopherHubbard/rxjs-custom-operators/issues"
  },
  "homepage": "https://github.com/ChristopherHubbard/rxjs-custom-operators#readme",
  "dependencies": {
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-form": "^8.1.0",
    "redux-observable": "^1.2.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.5.4",
    "ts-loader": "^5.3.3",
    "webpack": "^4.29.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@types/react-redux": "^7.0.1",
